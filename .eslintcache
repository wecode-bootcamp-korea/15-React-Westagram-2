od: 'GET'\n    }).then(response => response.json())\n      .then(data => {\n        this.setState({\n          users: selectIndex(24,5).map((item) => data.userInfos[item])\n        })\n      })\n  }\n\n  followUser = (id) => {\n    const newFollowingList = this.state.users.map((user) => {\n      if (user.userId === id) {\n        user.isFollowing = !user.isFollowing\n      }\n      return user\n    })\n    this.setState({users: newFollowingList})\n  }\n\n  render() {\n    return (\n      <div className='Aside'>\n        <div className='main-right-inner'>\n          <div className='main-right__my-account-container'>\n            <div className='main-right__my-profile-container'>\n              <img alt='My profile' src='images/eunjinlee/myprofile.JPG' />\n            </div>\n            <div className='main-right__my-profile-info-box'>\n              <a>workoutbutlazy</a>\n              <p>이은진 Eunjin Lee</p>\n            </div>\n            <a href='#' className='main-right__switch'>\n              Switch\n            </a>\n          </div>\n          <div className='main-right__suggestions-container'>\n            <div className='main-right__suggestions-title'>\n              <p>Suggestions For You</p>\n              <a>See All</a>\n            </div>\n            <ul\n              className='main-right__suggestion-list'\n              id='suggestionContainer'\n            >\n              {this.state.users.map((user) => {\n                return (\n                  <Recommend \n                    key={user.userId}\n                    id={user.userId}\n                    userId={user.userId}\n                    newStory={user.newStory}\n                    profile={user.profile}\n                    status={user.status}\n                    isFollowing={user.isFollowing}\n                    followUser={this.followUser}/>\n                )\n              })}\n            </ul>\n          </div>\n          <footer>\n            <p>\n              About Help Press API Jobs Privacy Terms Locations Top Accounts\n              Hashtags Language\n            </p>\n            <p>© 2020 INSTAGRAM FROM FACEBOOK</p>\n          </footer>\n        </div>\n      </div>\n    )\n  }\n}\n\nexport default Aside\n","/Users/reginalee/Desktop/wecode/westagram/src/Pages/eunjinlee/Main/Feeds/Feeds.js",["76","77","78","79"],"/Users/reginalee/Desktop/wecode/westagram/src/Pages/eunjinlee/Main/Comments/Comments.js",[],"/Users/reginalee/Desktop/wecode/westagram/src/Pages/eunjinlee/Main/Main.js",[],"/Users/reginalee/Desktop/wecode/westagram/src/Pages/eunjinlee/Login/Login.js",[],"/Users/reginalee/Desktop/wecode/westagram/src/Pages/eunjinlee/Main/UserInfos.js",[],"/Users/reginalee/Desktop/wecode/westagram/src/Components/Nav/Search/Search.js",["80","81"],"import React from 'react'\nimport './Search.scss'\nimport UserInfos from '../../../Pages/eunjinlee/Main/UserInfos'\n\n\nclass Search extends React.Component {\n\n  constructor() {\n    super()\n    this.state = {\n      searchList: [],\n      value: '',\n    }\n  }\n\n  getInputValue = (e) => {\n    this.setState({\n      value: e.target.value\n    })\n  }\n\n\n  render() {\n    const isOpenContainer = this.state.value\n    const isActiveInput = this.state.value\n    const searchText = this.state.value\n    const filteredList = UserInfos.filter((userInfo) => {\n      if (userInfo.userId.toLowerCase().includes(searchText.toLowerCase())\n        || userInfo.name.toLowerCase().includes(searchText.toLowerCase())) {\n        return userInfo\n      }\n    })\n\n    return(\n      <div className='Search'>\n        <input\n          type='text'\n          className={isActiveInput ? 'active' : ''}\n          placeholder='Search'\n          onChange={this.getInputValue}/>\n        <i className='fas fa-search'></i>\n        <ul\n          className={!isOpenContainer || filteredList.length === 0 ? 'search-list-container' : 'search-list-container open' }\n          id='search-list-container'>\n          {\n   